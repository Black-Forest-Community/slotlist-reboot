{
  "extends": "eslint-config-vue",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "rules": {
    "semi": ["error", "never"],
    "indent": ["error", 2],
    "quotes": ["error", "single"],
    "no-unused-vars": "warn"
  }
}
